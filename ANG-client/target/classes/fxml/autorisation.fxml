<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>


<Pane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Game.visual.Controller">
   <children>
      <TextField fx:id="playerName" promptText="Enter nickname" />
      <PasswordField fx:id="playerPassword" layoutY="25.0" promptText="Enter password" />
      <Button fx:id="acceptButton" alignment="CENTER" contentDisplay="CENTER" layoutX="24.0" layoutY="48.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0" text="Ok" />
   </children>
</Pane>
